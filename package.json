{
  "name": "screener-runner",
  "version": "0.9.0",
  "description": "Test Runner for Screener.io Visual Testing service",
  "repository": {
    "type": "git",
    "url": "https://github.com/screener-io/screener-runner.git"
  },
  "bin": {
    "screener-runner": "./bin/screener-runner.js"
  },
  "scripts": {
    "test": "npm run lint && nyc mocha \"test/**/*.spec.js\" && npm run coverage",
    "lint": "eslint .",
    "coverage": "nyc check-coverage --statements 90 --functions 90 --branches 80 --lines 90"
  },
  "engines": {
    "node": ">= 4"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/gzip-proxy.js"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "bluebird": "~3.4.6",
    "colors": "~1.1.2",
    "commander": "~2.9.0",
    "compression": "~1.6.2",
    "connect": "~3.5.0",
    "http-proxy": "~1.16.2",
    "joi": "~9.2.0",
    "lodash": "~4.16.6",
    "ngrok": "screener-io/ngrok",
    "portfinder": "~1.0.10",
    "request": "~2.78.0",
    "request-promise": "~4.1.1"
  },
  "devDependencies": {
    "chai": "~3.5.0",
    "eslint": "~3.11.1",
    "mocha": "~3.1.2",
    "nock": "~9.0.2",
    "nyc": "~8.4.0",
    "rewire": "~2.5.2",
    "sinon": "^1.17.6"
  }
}
